{% extends 'base.html.twig' %}

{% block title %}SubCategory
{% endblock %}

{% block body %}
	<div class="container containerForm">
		<div class="card p-4 col-6">
			<h1>SubCategory</h1>


			<table class="table">
				<tbody>
					<tr>
						<th>Id</th>
						<td>{{ sub_category.id }}</td>
					</tr>
					<tr>
						<th>Name</th>
						<td>{{ sub_category.name }}</td>
					</tr>
				</tbody>
			</table>
			<a href="{{ path('app_sub_category_edit', {'id': sub_category.id}) }}" class="btn btn-dark mb-2">Edit</a>

			{{ include('sub_category/_delete_form.html.twig') }}
			<a href="{{ path('app_sub_category_index') }}">back to list</a>
		</div>
	</div>
