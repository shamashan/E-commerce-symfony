{% if pageCount > 1 %}
	<nav aria-label="...">
		<ul class="pagination justify-content-center">

			{% if previous is defined %}
				<li class="page-item">
					<a href="{{path(route,query|merge({(pageParameterName):previous}))}}" class="page-link">Previous</a>
				</li>
			{% endif %}
			{% for page in pagesInRange %}
				{% if page != current %}
					<li class="page-item active" aria-current="page">
						<a class="page-link" href="{{path(route,query|merge({(pageParameterName):page}))}}">{{page}}</a>
					</li>
				{% else %}
					<li class="page-item active" aria-current="page">
						<a style="background-color: #9badfcff" class="page-link" href="{{path(route,query|merge({(pageParameterName):page}))}}">{{page}}</a>
					</li>
				{% endif %}
			{% endfor %}
			{% if next is defined %}
				<li class="page-item">
					<a class="page-link" href="{{path(route,query|merge({(pageParameterName):next}))}}">Next</a>

				</li>
			{% endif %}
		</ul>
	</nav>
{% endif %}
